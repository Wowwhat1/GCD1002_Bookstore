{% extends 'base.html.twig' %}

{% block title %}Publisher
{% endblock %}

{% block body %}
	<div class="container">

		<h1>Publisher</h1>

		<table class="table">
			<tbody>
				<tr>
					<th>Name</th>
					<td>{{ publisher.name }}</td>
				</tr>
				<tr>
					<th>Address</th>
					<td>{{ publisher.Address }}</td>
				</tr>
				<tr>
					<th>Phone</th>
					<td>{{ publisher.Phone }}</td>
				</tr>
			</tbody>
		</table>

		<a class="btn btn-dark" href="{{ path('app_publisher_index') }}">back to list</a>


		<a class="btn btn-dark" href="{{ path('app_publisher_edit', {'id': publisher.id}) }}">edit</a>


		{{ include('publisher/_delete_form.html.twig') }}</div>
{% endblock %}
